#vista_seleccion
  width:100%
  position:fixed
  background-image url(../img/bg_intro.jpg);
  background-position center center
  background-size cover
  left:0px
  top:0px
  z-index:9999
  height 100vh
  .content
    margin 0 auto
    text-align center
    color  #fff
    max-width 960px
    display block
    position relative
    h6
      font-family: Conv_VersaillesLTStd-Roman
      font-size: 12px
      margin-top: 12px
      text-transform uppercase
      letter-spacing 1px
    h5
      font-family: "Conv_SIMPLIFICA Typeface"
      font-size: 25px
      margin-top: 36px
      letter-spacing 2px
    ul
      max-width: 200px;
      overflow hidden
      margin: 78px auto 47px;
      font-family: "Conv_VersaillesLTStd-Roman"
      display block
      font-size: 20px;
      li
        cursor pointer
        font-family "Conv_VersaillesLTStd-Roman"
        a
          font-family "Conv_VersaillesLTStd-Roman"
          color #fff
        &:hover
          text-decoration underline
        &.si
          float left
        &.no
          float right

  .advertencia
    position absolute
    bottom 30px
    font-family: "Conv_SIMPLIFICA Typeface"
    font-size: 31px
    color #808080
    letter-spacing 3px
    margin 0 auto
    width  100%
    text-align center
#pre-load-web
  width:100%
  position:fixed
  background-image url(../img/bg_intro.jpg);
  background-position center center
  background-size cover
  left:0px
  top:0px
  z-index:100000
  #imagen-load
    left: 45%
    margin-left: -10px
    position: absolute
    display: block
    margin-bottom: 30px
    #logo_madera
      display: block
      margin-bottom  30px
    #progreso
      -webkit-animation: pace-spinner 1000ms linear infinite
      -moz-animation: pace-spinner 1000ms linear infinite
      -ms-animation: pace-spinner 1000ms linear infinite
      -o-animation: pace-spinner 1000ms linear infinite
      animation: pace-spinner 1000ms linear infinite
      display: block;
      margin-bottom: 30px;
      margin-left auto
      margin-right  auto

@keyframes pace-spinner
  0% { transform: rotate(0deg); transform: rotate(0deg); }
  100% { transform: rotate(360deg); transform: rotate(360deg); }

#header_responsive
  display none
header
  position fixed
  top 0px
  width 100%
  z-index 10
  .logo
    position absolute
    top 40px
    left 50px
    #titulo_logo
      transition 0.5s ease all
  .menu
    .btn_menu,.btn_menu_responsive
      position absolute
      top 50px
      right 45px
      cursor pointer
      width 61px
      height 57px
      background-image url(../img/navegacion.png);
      &:hover
        background-position -63px -3px
    aside
      position: absolute
      background-color: rgba(255, 255, 255, 0.94)
      right 0px
      height 100vh
      width: 120px
      nav
        margin 0 auto
        display block
        max-width 78px
        li
          margin-bottom: 65%
          a
            text-align center
            display block
            text-decoration none
          span
            font-family: Conv_SIMPLIFICA Typeface
            color: #666666
            font-size: 12px
            letter-spacing 1px
            text-align center
          .icon_menu
            background-image url(../img/navegacion.png)
            background-repeat: no-repeat
            display: block
            margin: 0 auto
            span
              line-height: 10px;
              display: block;
              vertical-align: bottom;
              position: relative;
          &.generaciones
            .icon_menu
              background-position: 1.8px -106px
              width: 55px;
              height: 30px;
              &:hover
                background-position: -69px -106px
              span
                line-height: 12px;
                bottom: -37px;
                right: -12px;
                margin-left: -24px;
          &.productos
            .icon_menu
              background-position: -7px -157px
              width: 41px
              height: 39px
              &:hover
                background-position -73px -157px
              span
                line-height: 12px;
                bottom: -45px;
          &.cocteleria
            .icon_menu
              background-position: -8px -215px;
              width: 42px;
              height: 30px;
              &:hover
                background-position -73px -215px
              span
                line-height: 12px;
                bottom: -34px;
          &.regalos
            margin-bottom: 70%
            .icon_menu
              background-position: -8px -297px;
              width: 44px;
              height: 37px;
              &:hover
                background-position -75px -297px
              span
                line-height: 12px;
                bottom: -39px;
                margin-left: -10px
                right: -5px
          &.ubicanos
            .icon_menu
              background-position: -8px -263px;
              width: 45px;
              height: 23px;
              &:hover
                background-position -73px -263px
              span
                line-height: 12px;
                bottom: -27px;
          &.social
            margin-top: 60px
            bottom: 0px;
            #youtube
              background-position: -8px -340px;
              width: 36px;
              height: 31px;
              background-image: url("../img/navegacion.png");
              background-repeat: no-repeat;
              display: inline-block;
              margin: 0 auto;
              &:hover
                background-position -74px -340px
              span
                line-height: 12px;
                bottom: -37px;
            #facebook
              background-position: -11px -373px;
              width: 33px;
              float: right
              height: 31px;
              background-image: url("../img/navegacion.png");
              background-repeat: no-repeat;
              display: inline-block;
              &:hover
                background-position -76px -373px

        .close_menu
          width 30px
          cursor pointer
          height 30px
          background-image url(../img/navegacion.png)
          background-repeat: no-repeat
          background-position 3px -61px
          margin 50px auto
          display block
          &:hover
            background-position -65px -63px

